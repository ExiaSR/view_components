<%= render Primer::BaseComponent.new(**@kwargs) do %>
  <% if heading %>
    <%= render Primer::BaseComponent.new(**heading.kwargs) do %>
      <%= heading.content %>
    <% end %>
  <% end %>
  <% items.each do |item| %>
    <%= render Primer::BaseComponent.new(**item.kwargs) do %>
      <%= render Primer::OcticonComponent.new(icon: item.icon) if item.icon.present? %>
      <%= item.label %>
    <% end %>
  <% end %>
<% end %>
